{
  "name": "@teimurjan/website",
  "version": "0.0.0",
  "private": true,
  "description": "Personal website built with Next",
  "author": "Teimur Gasanov",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "generate": "yarn generate-resume && yarn generate-opengraph && yarn generate-embeddings",
    "generate-resume": "tsx --tsconfig ../resume/tsconfig.json ./cmd/generate-resume.ts",
    "generate-opengraph": "tsx ./cmd/generate-opengraph.ts",
    "generate-opengraph:ci": "xvfb-run --auto-servernum tsx ./cmd/generate-opengraph.ts",
    "generate-embeddings": "tsx --env-file=.env ./cmd/generate-embeddings.ts"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.4.1",
    "@octokit/rest": "^22.0.0",
    "@octokit/types": "^14.1.0",
    "@teimurjan/github-client": "workspace:*",
    "@teimurjan/gql-client": "workspace:*",
    "@teimurjan/resume": "workspace:*",
    "@teimurjan/utils": "workspace:*",
    "@vercel/node": "^5.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "gsap": "^3.14.2",
    "lucide-react": "^0.477.0",
    "next": "15.2.4",
    "ogl": "^1.0.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.0",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@biomejs/js-api": "^4.0.0",
    "@biomejs/wasm-nodejs": "^2.3.13",
    "@next/bundle-analyzer": "^15.2.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0",
    "playwright": "^1.50.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^4",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "packageManager": "yarn@4.4.0"
}
